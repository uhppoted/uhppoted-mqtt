{
  "restore_default_parameters": {
    "command": "restore-default-parameters",
    "request": {
      "topic": "device:reset",
      "fields": [
        {
          "field": "device-id",
          "value": "uint32",
          "description": "(required) controller serial number"
        }
      ]
    },
    "response": {
      "fields": [
        {
          "field": "device-id",
          "value": "<controller-id>",
          "description": "controller serial number"
        },
        {
          "field": "reset",
          "value": "bool",
          "description": "reset controller success/fail"
        }
      ]
    }
  }
}
